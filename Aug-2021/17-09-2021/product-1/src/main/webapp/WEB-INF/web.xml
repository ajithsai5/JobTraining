<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>FindController</servlet-name>
  	<display-name>FindController</display-name>
  	<description></description>
  	<servlet-class>com.employee.controllers.FindController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetTheData</servlet-name>
  	<display-name>GetTheData</display-name>
  	<description></description>
  	<servlet-class>com.employee.controllers.GetTheData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteController</servlet-name>
  	<display-name>DeleteController</display-name>
  	<description></description>
  	<servlet-class>com.employee.controllers.DeleteController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateController</servlet-name>
  	<display-name>UpdateController</display-name>
  	<description></description>
  	<servlet-class>com.employee.controllers.UpdateController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetData</servlet-name>
  	<display-name>GetData</display-name>
  	<description></description>
  	<servlet-class>com.employee.controllers.GetData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateData</servlet-name>
  	<display-name>UpdateData</display-name>
  	<description></description>
  	<servlet-class>com.employee.controllers.UpdateData</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FindController</servlet-name>
  	<url-pattern>/FindController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetTheData</servlet-name>
  	<url-pattern>/GetTheData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteController</servlet-name>
  	<url-pattern>/DeleteController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateController</servlet-name>
  	<url-pattern>/UpdateController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetData</servlet-name>
  	<url-pattern>/GetData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateData</servlet-name>
  	<url-pattern>/UpdateData</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <jsp-property-group>
        <url-pattern>*.jsp</url-pattern>
        <el-ignored>false</el-ignored>
    </jsp-property-group>
</jsp-config>
</web-app>
